meňak neskutočné numeralio x = 4
kým x <= 10 rob
    ciskaj x
    x = x + 1

meňak neskutočné numeralio y = 1
kým y <= 10 rob
    meňak neskutočné numeralio z = y * y
    ciskaj z
    y = y + 1

meňak neskutočné numeralio d = 1
kým d <= 10 rob
    keď d % 2 == 0 potom
        ciskaj z
    d = d + 1
